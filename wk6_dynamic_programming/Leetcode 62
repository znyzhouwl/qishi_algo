class Solution(object):
    def uniquePaths(self, m, n):
        """
        :type m: int
        :type n: int
        :rtype: int
        """
        if m == 1 or n == 1:
            return 1
        bar = { (i,j):1 for i in range(n) for j in range(m) }
        for i in range(1,m):
            for j in range(1,n):
                bar[(j,i)] = bar[(j,i-1)] + bar[(j-1,i)]
        return bar[(n-1,m-1)]
