class Solution(object):
    def peakIndexInMountainArray(self, A):
        """
        :type A: List[int]
        :rtype: int
        """
        start, end = 0, len(A) - 1
        while start < end - 1:
            mid = (start + end)//2
            if A[mid] > A[mid - 1]:
                start = mid
            else:
                end = mid
        if A[start] >= A[end]:
            return start
        else:
            return end
